/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
    "compilerOptions": {
        "resolveJsonModule": true,
        "alwaysStrict": true,
        "experimentalDecorators": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2019",
            "ESNext",
            "ES2016"
        ],
        "module": "CommonJS",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "assets":[ "assets/*" ],
            "app": [ "src/app/*" ],
            "data": [ "src/app/data/*" ],
            "services":[ "src/app/services/*" ],
            "app-module": [ "src/app/modules/app-module/*" ],
            "browser-module": [ "src/app/modules/browser-module/*" ],
            "media-module": [ "src/app/modules/media-module/*" ],
            "*": [ "node_modules/*" ]
        }
    },
    "include": [
        "src/**/*"
, "assets/modules"    ],
    "exclude": [
        "ae-tmp/*/**",
    ]
}
